<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BaseToolbar">
        <attr name="bt_icon" format="integer|reference" />
        <attr name="bt_title" format="string" />
    </declare-styleable>

    <declare-styleable name="BaseToolbarMessage">
        <attr name="btm_icon_left" format="integer|reference" />
        <attr name="btm_avatar" format="integer|reference" />
        <attr name="btm_background" format="integer|reference" />
        <attr name="btm_icon_right_one" format="integer|reference" />
        <attr name="btm_icon_right_two" format="integer|reference" />
        <attr name="btm_icon_right_three" format="integer|reference" />
        <attr name="btm_main_name" format="string" />
    </declare-styleable>
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>

    <declare-styleable name="BaseRowMainProfile">
        <attr name="rowImage" format="integer|reference" />
        <attr name="rowItemDetail" format="string" />
        <attr name="rowItemEnableDivider" format="boolean" />
        <attr name="rowItemEnableNavigate" format="boolean" />
    </declare-styleable>

    <declare-styleable name="BaseRowProfile">
        <attr name="rowTitle" format="string" />
        <attr name="rowDetail" format="string" />
        <attr name="rowEnableDivider" format="boolean" />
    </declare-styleable>

</resources>